{
    "pipeline": {
        "roleArn": "<%= @CodePipelineRoleArn %>", 
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "inputArtifacts": [],
                        "name": "Checkout",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "version": "1",
                            "provider": "S3"
                        },
                        "outputArtifacts": [
                            {
                                "name": "MyApp"
                            }
                        ],
                        "configuration": {
                            "S3Bucket": "<%= @DevopsBucketName %>",
                            "S3ObjectKey": "test.zip"
                        },
                        "runOrder": 1
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "inputArtifacts": [
                            {
                                "name": "MyApp"
                            }
                        ],
                        "name": "Build",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "Custom",
                            "version": "<%= @VersionString %>",
                            "provider": "Jenkins"
                        },
                        "outputArtifacts": [
                            {
                                "name": "MyAppBuild"
                            }
                        ],
                        "configuration": {
                            "ProjectName": "MyDemoProject"
                        },
                        "runOrder": 2
                    }
                ]
            },
	    {
                "name": "Deploy", 
                "actions": [
                    {
                        "inputArtifacts": [
                            {
                                "name": "MyApp"
                            }
                        ], 
                        "name": "DeployWordpress", 
                        "actionTypeId": {
                            "category": "Deploy", 
                            "owner": "AWS", 
                            "version": "1", 
                            "provider": "CodeDeploy"
                        }, 
                        "outputArtifacts": [], 
                        "configuration": {
                            "ApplicationName": "wordpress_app", 
                            "DeploymentGroupName": "wordpress_depgroup"
                        }, 
                        "runOrder": 1
                    }
                ]
            }
        ], 
        "artifactStore": {
            "type": "S3", 
            "location": "<%= @DevopsBucketName %>"
        }, 
        "name": "<%= @CodePipelineName %>", 
        "version": 1
    }
}
