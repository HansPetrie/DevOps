{
    "category": "Build", 
    "provider": "JenkinsProvider", 
    "version": "VERSION_STRING", 
    "settings": {
        "entityUrlTemplate": "http://JENKINS_SERVER/job/{Config:ProjectName}", 
        "executionUrlTemplate": "http://JENKINS_SERVER/job/{Config:ProjectName}/{ExternalExecutionId}"
    }, 
    "configurationProperties": [
        {
            "name": "ProjectName", 
            "required": true, 
            "key": true, 
            "secret": false, 
            "queryable": true
        }
    ], 
    "inputArtifactDetails": {
        "minimumCount": 0, 
        "maximumCount": 5
    }, 
    "outputArtifactDetails": {
        "minimumCount": 0, 
        "maximumCount": 5
    }
}
